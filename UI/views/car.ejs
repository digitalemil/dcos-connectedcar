		function saveImg() {
			var dataURL = canvas.toDataURL('image/png');
			window.open(dataURL);
		};

		function handleKey(event) {
			if (event.keyCode == 189)
				biz.zoomOut();
			if (event.keyCode == 187)
				biz.zoomIn();
		}

		noPhoneGapSetup = function() {
			if (w == undefined)
				setup();
		}

		var deviceReady = function() {
			setTimeout(setup, 200);
		}
		init = function() {
			document.addEventListener("deviceready", deviceReady, true);
		}

		setup = function() {
			w = window.innerWidth;
			h = window.innerHeight;

			canvaswidth = w;
			canvasheight = h;

			startMe();
		}

		sign = function(i) {
			if (i < 0)
				return -1;
			if (i > 0)
				return 1;
			return 0;
		}

		var thing;
		var latitude= "0";
		var longitude= "0";
		var altitude= "0";
		var heading= "0";
		var gpsspeed= "0";
		


		function rotateThing() {
			thing.rotate(6);
			setTimeout(rotateThing, 300);
		}

		function onGeoSuccess(position) {
                        latitude = position.coords.latitude;
                        longitude = position.coords.longitude;
						altitude = position.coords.altitude;
						heading = position.coords.heading;
						gpsspeed = position.coords.speed;
              		console.log("Geo success...");
        
	  };

        function onGeoError(error) {
                		console.log("Geo error...");
        };
	    
	

		startMe = function() {
			w = window.innerWidth;
			h = window.innerHeight;

			scale = Math.min((w) / 640, (h) / 960);
			document.getElementById("canvas").setAttribute("style",
					"position:absolute; top:" + 0 + "px; left: " + 0 + "px;");
			document.getElementById("canvas")
					.setAttribute("width", canvaswidth);
			document.getElementById("canvas").setAttribute("height",
					canvasheight);

